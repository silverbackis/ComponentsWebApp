{
  "name": "components-web-app",
  "license": "MIT",
  "version": "0.0.1",
  "description": "Nuxt.js project - Using CWAModules which provide Nuxt modules, and API Component Bundle, this package demonstrates how you can acheive a component-driven progressive web app using the latest modern technologies.",
  "author": "Daniel West <daniel@silverback.is>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt build && nuxt start",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "format": "prettier --no-semi --single-quote --write '{,!(node_modules)/**/}*.{js,json,jsx,md,vue}'",
    "test": "jest --projects --testRegex '_tests/.*\\.spec\\.js$'",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@cwamodules/bulma": "^0.5.2",
    "@cwamodules/server": "^0.5.1",
