    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/component-cache": "^1.1.3",
    "@nuxtjs/google-tag-manager": "^2.1.0",
    "@nuxtjs/pwa": "^2.6.0",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-mysql-session": "^2.1.0",
    "express-session": "^1.15.6",
    "flatpickr": "^4.5.4",
    "helmet": "^3.14.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "nuxt": "^2.4.5",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.17"
